<div class="d-flex mt-3" aria-label="Table navigation">

        <ul class="pagination ms-3">
            <li class="page-item">
                <a class="page-link <%- page == 1 ? "disabled" : "" %>" href="/dashboard?page=<%- page - 1 %>"
                    aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                </a>
            </li>
            <% for( let index=iterator; index < endingLink; index++ ) { %>
            <li class="page-item">
                <% if(index === page) { %>
                    <a class="page-link active" href="/dashboard?page=<%=index%>"><%=index%></a>
                    <% continue; %>
                <% } %> 
                <a class="page-link"
                    href="/dashboard?page=<%- index %>">
                    <%- index %>
                </a>
            </li>
            <% } %>
    
            <li class="page-item">
                <a class="page-link <%- page >= endingLink ? "disabled" : "" %>" href="/dashboard?page=<%- page + 1 %>"
                    aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
            </li>
        </ul>

